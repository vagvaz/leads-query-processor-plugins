<script type="text/javascript">

function doManyTimes(funcName,delay,num){
  var time=0;
  for (var i = 0; i < num; i++) {
    time += delay;
    setTimeout(function(j) {
      return function() {funcName(j)}
    }(i), time);
  }
}

/////////////////////////////////
// PLP Carousel updater
/////////////////////////////////

// only do this on PLPs
if($('body#searchResultPage').length){

  // only do this on PLPs with carousel active
  if($('div#carousel').length && resx.rrec==true){

    resx.tilesDone=0;
    resx.theTiles={};
    resx.arrowsDone=0;

    resx.improveCarousel = function(x){
      if(resx.tilesDone==0){
        if($('#carousel.infiniteCarousel #Carouselfinal li a').length!=0){
          resx.theTiles=$('#carousel.infiniteCarousel #Carouselfinal li a');
          resx.theTiles.each(function(index){
            $(this).attr('manual_cm_sp','RECS_PLP_CAROUSELS-_-Certona_Scheme_'+resx.rrelem+'-_-product_slot_'+index);
          });
          resx.tilesDone=1;
        }
      }
      if(resx.arrowsDone==0){
        if($('#carousel.infiniteCarousel a.arrow').length!=0){
          $('#carousel.infiniteCarousel a.arrow.back').click(function() {
            tagPageElement('RECS:PLP_CAROUSELS','RECS_PLP_CAROUSELS-_-Certona_Scheme_'+resx.rrelem+'_prev');
          });
          $('#carousel.infiniteCarousel a.arrow.forward').click(function() {
            tagPageElement('RECS:PLP_CAROUSELS','RECS_PLP_CAROUSELS-_-Certona_Scheme_'+resx.rrelem+'_next');
          });
          resx.arrowsDone=1;
        }
      }
    }
    // call the the improveCarousel func 16 times 320 millisecs apart
    doManyTimes(resx.improveCarousel,320,16);
  }
}
</script> 	
			
