					<a id="cartLogo" href="/us/cart/show" manual_cm_sp="global_header-_-top_right-_-cart_logo"><img src="/us/cms_content/adi/us/en/sitecontent/common/media/adus_headerCartBG.gif" width="34" height="35" alt="adidas shopping bag" title=""/></a>

					<script language="Javascript" type="text/javascript">
					//show logout link for logged in user
					var userLoggedIn = getCookie('LG_F');
					if(userLoggedIn !== false && userLoggedIn.replace('"','') === "lf===1"){
						document.getElementById("logout").className = "show";
					}
					// read cart count from cookie

					var cart_cookie= getCookie('sa_P');
					var cart_count = getCartCount(cart_cookie);

					if(cart_count=='1')	{
						$('#cartItmesDiv .cart-count').text(cart_count+' item');
					}else{
						$('#cartItmesDiv .cart-count').text(cart_count+' items');
					}

					</script>
