			<a href="" id="topper-tag-btn" name="topper-tag-btn" manual_cm_sp="global_header-_-top_right-_-topper_tag"><img id="topper-tag-img" align="right" src="/us/cms_content/adi/us/en/marketingcontent/home/media/free_ship_return_btn.jpg" width="250" height="26" alt="Free Shipping + Free Returns" /></a>

			<script type="text/javascript">

				$(function(){
					var container = $('#topper-thing'),
						transforms = ( typeof Modernizr !== 'undefined' && Modernizr.csstransforms3d ) ? true : false;

					if(!container.length) return;

					container.removeAttr('style');

					container.bind('close',function(){
						if( transforms ) {
							container.removeClass('open');
						} else {
							container
								.removeClass('opening')
								.stop()
								.slideUp(function(){
									$(this).removeClass('open')
								});
						}
					});
					container.bind('open',function(){
						if( transforms ) {
							container.addClass('open');
						} else {
							container
								.addClass('opening')
								.stop()
								.slideDown('slow',function(){
									$(this)
										.removeClass('opening')
										.addClass('open');
								});
						}
					});
					$('#topper-tag-btn').bind('click',function(){
						if( container.hasClass('open') || container.hasClass('opening') ) {
							container.trigger('close');
							tagPageElement('HEAD:EXPAND',topper_btn_name+'_close');
						} else {
							container.trigger('open');
							tagPageElement('HEAD:EXPAND',topper_btn_name+'_open');
						}
						return false;
					});
				});

				$(function(){
				if(topper_pop_active==1){
				if(topper_cookie_value=='1'){
				}else{
					var topperTag = $('#topper-tag-img');
					// topperTag.attr('src',topperTag.attr('src').replace('_opener.','_closer.'));

					var container = $('#topper-thing');
					container.trigger('open');
					SetCookie(topper_cookie_name,"1",topper_cookie_days);
					tagPageElement('HEAD:EXPAND',topper_btn_name+'_auto_open');
				}
				}
				});

			</script>

		</div>

