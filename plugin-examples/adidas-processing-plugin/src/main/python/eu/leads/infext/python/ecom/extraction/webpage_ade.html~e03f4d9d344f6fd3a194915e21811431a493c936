var clicked = false;
$('iframe[src^="http://www.adidas.com/video"]').live('mouseover', function(){
	clicked = true;
  	setTimeout(function() { 
    	if (clicked) { 
			cmCreateElementTag("Play", "PDP: Video");
		}
	}, 1500);
}).live('mouseout', function(){
  clicked = false;
});
</script>
