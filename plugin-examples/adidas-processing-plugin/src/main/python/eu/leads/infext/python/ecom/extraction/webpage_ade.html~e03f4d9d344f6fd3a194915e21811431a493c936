<script>
	$('a.video-dialog').click(function(event) {
		event.preventDefault();	
				
		app.dialog.open(
			this.href+((this.href.indexOf('?') > 0)?'&view=ajax':'?view=ajax'), $(this).text(), { 
				"width":853, 
				"height":500, 
				"dialogClass":"video", 
				"close": function(event, ui){
					jQuery(this).dialog("destroy")
				}
			}
		);
	});
</script>	
		
	
