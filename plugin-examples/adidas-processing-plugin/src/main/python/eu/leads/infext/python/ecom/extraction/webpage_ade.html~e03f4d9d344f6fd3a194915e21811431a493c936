		var w8time = 5000;
		var fired = [];
		
		function resourceLoader(bindfunc){
			var counter = 0;
			var maxeffort = 10;		
			return function loadercb(){
				if(!bindfunc() && counter++ < maxeffort){
					setTimeout(loadercb, w8time * Math.random());
				}
			}
		};
		
		
		(resourceLoader(function (){
			if('FB' in window){	
				FB.Event.subscribe('edge.create',function(response) {
					var platform = 'FACEBOOK_LIKE';
					if(!fired[platform]){
						var cmData;
						if( 'cmSwatchData' in app.ProductCache ){
							cmData = ['FACEBOOK: LIKE', null, [null,null,null,null,null,null,app.ProductCache.cmSwatchData.articleId]]; 
						}
						jQuery(document).trigger('fb-like.pdp',[cmData]);
						jQuery(document).trigger('certona.op', { event:'like', itemid:app.ProductCache.pid });
						jQuery(document).trigger("analytics.product-share", [ "FACEBOOK: LIKE" ]);
						fired[platform] = true;
					}
				});
				return true;
			}
		}))();

		(resourceLoader(function (){
			if('addthis' in window){
				addthis.addEventListener('addthis.menu.share',function(evt){
					var platform = String(evt.data.service).toUpperCase();
					if(!fired[platform]){
						var cmData;
						if( 'cmSwatchData' in app.ProductCache ){
							cmData = ['' + platform, null, [null,null,null,null,null,null,app.ProductCache.cmSwatchData.articleId]]; 
						}
						jQuery(document).trigger('addthis-share.pdp', [cmData]);
						jQuery(document).trigger('certona.op', { event:(platform == 'TWEET' ? 'tweet' : 'share'), itemid:app.ProductCache.pid });
						jQuery(document).trigger("analytics.product-share", [ platform || "SHARE" ]);
						fired[platform] = true;
					}
				});
				return true;
			}
		}))();
		
