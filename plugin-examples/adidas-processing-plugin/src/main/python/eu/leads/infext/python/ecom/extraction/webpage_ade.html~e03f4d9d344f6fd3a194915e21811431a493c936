<script type="text/javascript" src="http://demandware.edgesuite.net/aagl_prd/on/demandware.static/Sites-adidas-DE-Site/Sites-adidas-DE-Library/de_DE/v1389580184673/brand/shared/carrousel/mastheadSlider/adi-onesite-mastheads.js"></script>






	
	<script type="text/javascript">
		var isMobile = false,	// Flag if currently this is a mobile version being displayed
			cookieName = 'MobileRedirectProcessed',												// Name of the cookie that will hold a flag for skipping further redirects - it should be done only once
			isUrlValid = app.util.isInWhiteLists();			// checks if domain not in global custom preference 'mobileRedirectBlackList' and url not in global custom preference 'redirectionBlackUrlsList'
		// Process redirect rules only if no cookie is set		
		if (!app.util.getCookie(cookieName)) {
			var mPrefix   = new RegExp('^\s*(https?://)?(.+?@)?m\\.', 'i');
			var wwwPrefix = new RegExp('^\s*(https?://)?(.+?@)?www\\.', 'i');

			// Redirect to desktop from mobile version if device is a tablet PC
			if (isMobile && MODETECT.device.tablet) {
			    window.location.hostname = window.location.hostname.replace(mPrefix, '$1$2www.');
			}
			// Redirect to desktop from mobile version if url contain part from array of redirectionBlackUrlsList
			else if (isMobile && !isUrlValid) {
			    window.location.hostname = window.location.hostname.replace(mPrefix, '$1$2www.');
			}
			
			// Redirect to mobile from desktop if device is a smaptphone
			else if (!isMobile && MODETECT.device.phone && isUrlValid && !window.location.hostname.match(mPrefix)) {
				window.location.hostname = "m.adidas.de" || window.location.hostname.replace(wwwPrefix, '$1$2m.');
			}
		}
	</script>




