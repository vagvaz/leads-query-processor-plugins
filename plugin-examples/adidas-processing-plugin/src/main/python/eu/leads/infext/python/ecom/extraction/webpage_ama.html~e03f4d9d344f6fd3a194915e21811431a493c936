          <# } #>
          <# if (item.scarcityMessage) { #>
            <span class='<#=item.scarcityClass #>'><#=item.scarcityMessage #></span>
          <# } #>
          <span class='nav-cart-quantity'>
            <# if(typeof item.wireless !== 'undefined') { #>
