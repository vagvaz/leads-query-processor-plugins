    addLogical:f,
    addStyle:f,
    addPL:f,
    available:f,
    chars:{EOL:ch(10), SQUOTE:ch(39), DQUOTE:ch(34), BACKSLASH:ch(92), YEN:ch(165)},
    completedStage:f,
    declareAvailable:f,
    onCompletion:f,
    onReady:f,
    strings:{}
  };
}());


