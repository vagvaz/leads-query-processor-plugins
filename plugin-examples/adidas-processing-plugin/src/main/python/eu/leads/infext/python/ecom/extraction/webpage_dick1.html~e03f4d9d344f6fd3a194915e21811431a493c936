		<script type="text/javascript">
            function getDomain(){
                var host = window.location.hostname;
                var spl = host.split(".");
                host = spl[spl.length-2]+"."+spl[spl.length-1];
                return host;
            };
            function setDisplayStyle(elementName, style) {
                if (document.getElementById(elementName) != null) {
                    document.getElementById(elementName).style.display = style;
                }
            };
            function setBVReviewsState(setStateTo){
                if (setStateTo=='show'){
                    setDisplayStyle('BVRRContainer','block');
                    document.getElementById("BVRatingsShowHideLink").innerHTML='Hide Reviews';
                    document.getElementById("BVRatingsShowHideLink").onclick=function(){setBVReviewsState('hide');};
                } else {
                    setDisplayStyle('BVRRContainer','none');
                    document.getElementById("BVRatingsShowHideLink").innerHTML='Read Reviews';
                    document.getElementById("BVRatingsShowHideLink").onclick=function(){setBVReviewsState('show');};
                }
            };
            function scrollToContainer(containerName){
                setBVReviewsState('show');
                location.href='http://www.dickssportinggoods.com/product/index.jsp?productId=19283636' + containerName;
            };
