					<script type="text/javascript">
						var clickedColor = null;
						var clickedSize = null;
						var distsizeobj=new Array();
					
						distsizeobj[0]="6.0";
					
						distsizeobj[1]="6.5";
					
						distsizeobj[2]="7.0";
					
						distsizeobj[3]="7.5";
					
						distsizeobj[4]="8.0";
					
						distsizeobj[5]="8.5";
					
						distsizeobj[6]="9.0";
					
						distsizeobj[7]="9.5";
					
						distsizeobj[8]="10.0";
					
						distsizeobj[9]="10.5";
					
						distsizeobj[10]="11.0";
					
						distsizeobj[11]="11.5";
					
						distsizeobj[12]="12.0";
					
						distsizeobj[13]="13.0";
					
						distsizeobj[14]="14.0";
					
						distsizeobj[15]="15.0";
					
						distsizeobj[16]="16";
					
						var indexcolor=0;
						var indexsize=0;

						function DefaultSwatch(){
							changeswatch(0);
						}
						function respondToClick(event){
							var element = event.element();
							if(clickedColor != null){
								clickedColor.removeClassName('clicked');
							}
							if(clickedColor != element && clickedSize != null){
								clickedSize.removeClassName('clicked');
							}
							
							clickedColor = element;
							clickedColor.addClassName('clicked');
							clickedprodShotImg = $$('img[name="prodShot"]')[0].src;
						}
						function respondToClick2(event){
							var element2 = event.element();
								
								
								if(clickedSize != null){
									clickedSize.removeClassName('clicked');
								}
								clickedSize = element2;
								clickedSize.addClassName('clicked');
						}
						function changeswatch(p){
							$("color-name").innerHTML = "<span class=\"icon1_bullets icon_bullets\" id=\"icon1_bullets\">1</span><span class=\"optionHeading\">COLOR: <span class=\"capitalize\">" +skuSizeColorObj[p].ColorDesc.toLowerCase().replace("/", " / ")+ "</span></span>";
							$("swatch-img"+p).setAttribute("title",skuSizeColorObj[p].ColorDesc.toLowerCase().replace("/", " / "));
							$("swatch-img"+p).setAttribute("alt",skuSizeColorObj[p].ColorDesc.toLowerCase().replace("/", " / "));
							$("swatch-img"+p).observe('click', respondToClick);
							var testing = $("swatch-img"+p).src;
							if(testing.indexOf("no-image") > -1 && window.initialproShotImg){
								$$('img[name="prodShot"]')[0].src = initialproShotImg;
							}

							indexcolor=p;

							for(i=0;i<distsizeobj.length;i++){
								var si=distsizeobj[i];
								$("button"+si).disabled=true;
								
								$("button"+si).removeClassName("available");
								$("button"+si).addClassName("unavailable");
								$("button"+si).setAttribute("title","Item is Unavailable");
								
								$("button"+si).observe('click', respondToClick2);
							}
							for (i=0; i<skuSizeColorObj[p].sizeObject.length; i++){
								var si=skuSizeColorObj[p].sizeObject[i].sizeDesc;
								var n= distsizeobj.indexOf(si);
							
								if(n != -1){
									$("button"+si).disabled=false;
									$("button"+si).addClassName("available");
									$("button"+si).removeClassName("unavailable");
									$("button"+si).setAttribute("title",si);
									$("button"+si).observe('click', respondToClick2);
								}
							
							}

						}
						function priceChange(b){
							for (i=0; i<skuSizeColorObj[indexcolor].sizeObject.length; i++){
								if(b==skuSizeColorObj[indexcolor].sizeObject[i].sizeDesc)
								{
                                var price = skuSizeColorObj[indexcolor].sizeObject[i].skuPrice;
                                if(price != "")
							{
									var divArray = document.getElementsByTagName("div");
                                if (divArray.length > 0){
								for (y = 0; y < divArray.length; y++){
									if (divArray[y].className == "op"){

													divArray[y].innerHTML = 'Price: '+'&#036;' + price;


										}
									}
								}
							}
						}
				     }

                  }
						function pricereset() {
							if(price != "") {
								$$('.op')[0].innerHTML = onloadPrice;
							}
						}

						function gotoSelected() {
							$$('img[name="prodShot"]')[0].src = initialproShotImg;
							var swatch = $$('#colorSwatches li img');
							for(k=0; k < swatch.length; k++){
								if(swatch[k].hasClassName("clicked")){
									$("color-name").innerHTML = "<span class=\"icon1_bullets icon_bullets\" id=\"icon1_bullets \">1</span><span class=\"optionHeading\">COLOR: <span class=\"capitalize\">" +skuSizeColorObj[k].ColorDesc.toLowerCase().replace("/", " / ")+ "</span></span>";
									$("swatch-img"+k).setAttribute("title",skuSizeColorObj[k].ColorDesc.toLowerCase().replace("/", " / "));
									$("swatch-img"+k).setAttribute("alt",skuSizeColorObj[k].ColorDesc.toLowerCase().replace("/", " / "));
									$("swatch-img"+k).observe('click', respondToClick);
									var testing = $("swatch-img"+k).src;

									$$('img[name="prodShot"]')[0].src = clickedprodShotImg;
									indexcolor=k;

									for(i=0;i<distsizeobj.length;i++){
										var si=distsizeobj[i];
										$("button"+si).disabled=true;
										
										$("button"+si).removeClassName("available");
										$("button"+si).addClassName("unavailable");
										$("button"+si).setAttribute("title","Item is Unavailable");
										
										$("button"+si).observe('click', respondToClick2);
									}
									for (i=0; i<skuSizeColorObj[k].sizeObject.length; i++){
										var si=skuSizeColorObj[k].sizeObject[i].sizeDesc;
										var n= distsizeobj.indexOf(si);
									
										if(n != -1){
											$("button"+si).disabled=false;
											$("button"+si).addClassName("available");
											$("button"+si).removeClassName("unavailable");
											$("button"+si).setAttribute("title",si);
											$("button"+si).observe('click', respondToClick2);
										}
									

									}
								}
							}
						}
