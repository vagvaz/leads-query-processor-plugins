	function toggleProduct(){
		change(0);
	}
</script>

<style>
	#tabMenu td{background-image:url('/images/product_tab_space.jpg'); background-repeat:repeat-x; height:22px; text-align:center;}
	#tabMenu .on{background-image:url('/images/product_tab_spaceOn.jpg');}
	@media screen and (-webkit-min-device-pixel-ratio:0) { #selectQTY { margin-left: -5px !important; }}
</style>

<script type="text/javascript">
////////////////////////VARS/////////////////////////////////////////////////
//menu height
var menuwidth=238;
//scroll speed
var scrollspeed=6;
//leave blank
var actualwidth='';

////////////////////////FUNCTIONS/////////////////////////////////////////////////
//scrolls left
function moveleft(idDiv){
	$('sArrowR').removeClassName('inactive');
	theDiv = document.getElementById(idDiv);
	if (parseInt(theDiv.style.left)<0){
		theDiv.style.left=parseInt(theDiv.style.left)+scrollspeed+"px";
	} else {
		$('sArrowL').addClassName('inactive');
	}
	lefttime=setTimeout("moveleft('"+idDiv+"')",50)
}

//scrolls right
function moveright(idDiv, thewidth){
	$('sArrowL').removeClassName('inactive');
	theDiv = document.getElementById(idDiv);
	if (parseInt(theDiv.style.left)>(menuwidth-thewidth)){
		theDiv.style.left= parseInt(theDiv.style.left) - scrollspeed + 'px';
	} else {
		$('sArrowR').addClassName('inactive');
	}
	righttime=setTimeout("moveright('"+idDiv+"',"+thewidth+")",50)
}

function scrollBox(theId, theData){
	var theIdtemp = theId + 'temp';
	//make hidden layer to get the width of the content
	document.write('<div id="'+theIdtemp+'" class="tempLayer">'+theData+'</div>');
	//get the width of the temp layer
	//actualwidth = document.getElementById(theIdtemp).offsetWidth;
	var _items = document.getElementById(theIdtemp).getElementsByTagName("li");
	actualwidth = ((_items[0].offsetWidth+62) * _items.length) -10; 
	//decide to make scroll of not
	if(actualwidth > menuwidth ){
		//make scroll stuff	
		document.write('<div id="sArrowL" class="sArrowL inactive" onMouseover="moveleft(\'' + theId + '\')" onMouseout="clearTimeout(lefttime)"></div>')
        document.write('<div class="sMask" style="width:'+menuwidth+'px;">')
		document.write('<div class="sCon" id="'+theId+'" style="left:0px; width:' + actualwidth + 'px;">' + theData + '</div>')
		document.write('</div>')
		document.write('<div id="sArrowR" class="sArrowR" onMouseover="moveright(\'' + theId + '\','+ actualwidth +')" onMouseout="clearTimeout(righttime)"></div>')
	}else{
		//no scroll stuff
		
		document.write('<div style="height:50px; padding-left:18px;">' + theData + '</div>')
	}
}

////////////////////////////////////////////////////////////////////////

var tabCount = null;

function tabSet(){
	tabCount = tabArray.length
	var html = '<table id="tabMenu" cellspacing="0" cellpadding="0">'
	html += '<tr>'
	if(curTab == 0){
		leftImg = '/images/core_tab_leftOn.jpg'	
	}else{
		leftImg = '/images/core_tab_left.jpg'
	}
	html += '<td width="3"><img id="pictabL" src="'+leftImg+'"></td>'
	for(var i=0; i<tabCount; i++){
		if(i == curTab){
			classIt = "on"
			imgIt = tabArray[i]['onImg']
			rightOfImg = '/images/core_tab_rightOfOn.jpg'
		}else{
			classIt = "off"
			imgIt = tabArray[i]['offImg']
			rightOfImg = '/images/core_tab_sep.jpg'
		}
		if(i == (curTab - 1)){
			rightOfImg = '/images/core_tab_leftOfOn.jpg'
		}
		html += '<td id="tab'+i+'" class="'+classIt+'"><a href="javascript:void(0);" onclick="change('+i+')" onmouseover="hover('+i+',1)" onmouseout="hover('+i+',0)"><img id="pictab'+i+'" src="'+imgIt+'" width="144" height="22" alt=""></a></td>'		
		if(i == (tabCount-1)){
		}else{
			html += '<td width="5"><img id="pictabC'+i+'" src="'+rightOfImg+'"></td>'	
		}
	}
	if(curTab == (tabCount-1)){
		rightImg = '/images/core_tab_rightOn.jpg'		
	}else{
		rightImg = '/images/core_tab_right.jpg'
	}
	html += '<td width="3"><img id="pictabR" src="'+rightImg+'"></td>'
	html += '</tr>'
	html += '</table>'
	document.write(html)
}

function loadLayer(){
	document.getElementById(tabArray[curTab]['divId']).style.display = "block"
}

function hover(theId,theAction){
	var doWhat = null
	if(theAction == 1){
		doWhat = tabArray[theId]['aImg']
	}else if(theAction == 0){
		doWhat = tabArray[theId]['offImg']
	}
	var ifOn = document.getElementById('tab'+theId).className
	if(ifOn == 'off'){
		document.getElementById('pictab'+theId).src = doWhat
	}
}

function change(theId){
	//this is convoluted so it works in IE6 in quirks mode...
	turnOffImages(theId);
}

function turnOffImages(theId) {
	// turn off old stuff
	if(curTab == 0){
		document.getElementById('pictabL').src = '/images/core_tab_left.jpg'
		document.getElementById('pictabC'+curTab).src = '/images/core_tab_sep.jpg'
	}else if(curTab == (tabCount-1)){
		document.getElementById('pictabR').src = '/images/core_tab_right.jpg'
		document.getElementById('pictabC'+(curTab-1)).src = '/images/core_tab_sep.jpg'
	}else{
		document.getElementById('pictabC'+curTab).src = '/images/core_tab_sep.jpg'
		document.getElementById('pictabC'+(curTab-1)).src = '/images/core_tab_sep.jpg'
	}
	var turnOn = setTimeout(function(){turnOnImages(theId)},100);
}

function turnOnImages(theId) {
	//turn on new stuff
	document.getElementById('pictab'+theId).src = tabArray[theId]['onImg']
	document.getElementById('tab'+theId).className = "on"
	document.getElementById(tabArray[theId]['divId']).style.display = "block"
	if(theId == 0){
		document.getElementById('pictabL').src = '/images/core_tab_leftOn.jpg'
		document.getElementById('pictabC'+theId).src = '/images/core_tab_rightOfOn.jpg'
	}else if(theId == (tabCount-1)){
		document.getElementById('pictabR').src = '/images/core_tab_rightOn.jpg'
		document.getElementById('pictabC'+(theId-1)).src = '/images/core_tab_leftOfOn.jpg'
	}else{
		document.getElementById('pictabC'+theId).src = '/images/core_tab_rightOfOn.jpg'
		document.getElementById('pictabC'+(theId-1)).src = '/images/core_tab_leftOfOn.jpg'
	}
	document.getElementById('pictab'+curTab).src = tabArray[curTab]['offImg']
	document.getElementById('tab'+curTab).className = "off"
	document.getElementById(tabArray[curTab]['divId']).style.display = "none"
	curTab = theId
}
</script>



<!-- showSkuImages = false-->






 









<script>
	Event.observe(window, "load", function() {
        if ($$('.op')[0]){
            onloadPrice = $$('.op')[0].innerHTML;
            priceHeight = $$('.op')[0].clientHeight;
            $$('.op')[0].style.height = priceHeight + "px";
        }
	});
	
	function price(selectElement) {
	
		var selectedElementText = selectElement.options[selectElement.selectedIndex].text;
		var price = 0;
		var i,k,y = 0;
		var selectedIndex = selectedElementText.toString().indexOf("$");
		var length = selectedElementText.toString().length;
		
		if ( selectedIndex >-1) {
			price = selectedElementText.toString().slice(selectedIndex,length); 
			var divArray = document.getElementsByTagName("div");
			if (divArray.length > 0){
				for (y = 0; y < divArray.length; y++){
					if (divArray[y].className == "op"){
						divArray[y].innerHTML ='Price: '+ price;
					}
				}
			}
		} else {
			$$('.op')[0].innerHTML = onloadPrice;
		}
	}
	
</Script>








<script language="Javascript">
	var notToAdd = false;
	var notCartAllSku = "";

	if("".length > 0){
		notCartAllSku="";
	}
	function hideAddtoCart(singleskuid){
		if("".length > 0){
			if("".indexOf(singleskuid) > -1){
				notToAdd = true;
				eval("$('atc').style.visibility = \"" + 'hidden' + "\"");
				document.getElementById("notAvailDiv").innerHTML = '<font class=\"pagetext\"><b>Item not available in stock.</b></font>';
				eval("$('notAvailDiv').style.visibility = \"" + 'visible' + "\"");
				eval("$('warrantyDiv').style.visibility = \"" + 'hidden' + "\"");
				eval("$('prodVarDiv').style.visibility = \"" + 'hidden' + "\"");
			}
		}
	}
	function checkAvail(){
		var prod_n_sku = document.orderForm.prod_0.options[document.orderForm.prod_0.selectedIndex].value;
		if(prod_n_sku.indexOf("|") > -1){
			var arrayOfSkus = prod_n_sku.split("|");
			var skuToShow = arrayOfSkus[1];
			if(notCartAllSku.indexOf(skuToShow) > -1){
				notToAdd = true;
					document.getElementById("notAvailDiv").innerHTML = '<font class=\"pagetext\"><b>Item not available in stock.</b></font>';
					eval("$('notAvailDiv').style.visibility = \"" + 'visible' + "\"");
					eval("$('warrantyDiv').style.visibility = \"" + 'hidden' + "\"");
					eval("$('prodVarDiv').style.visibility = \"" + 'hidden' + "\"");
			} else {
				notToAdd = false;
				//commented to remove fluctuation on selection of any option in color combo
				//document.getElementById("notAvailDiv").innerHTML = '&nbsp;';
				eval("$('warrantyDiv').style.visibility = \"" + 'visible' + "\"");
				eval("$('prodVarDiv').style.visibility = \"" + 'visible' + "\"");
				eval("$('notAvailDiv').style.visibility = \"" + 'hidden' + "\"");
			}
		}
	}
	function showEnhanced(){
		//Set selected swatch index
		if($$('.capitalize')[0]) {
			var selColor = $$('.capitalize')[0].innerHTML.replace(/\s/g,"");
		}
		if(imageMap_0 && selColor) {
			for (var i=0; i < imageMap_0.length; i++) {
				if((selColor.toUpperCase()) == (imageMap_0[i].cname.toUpperCase().replace(/\s/g,""))) {
					enhValue = imageMap_0[i].enh;
					break;
				} else {
					enhValue = imageMap_0[0].enh;
				}
			}
		}
		if(true){
			if(document.getElementById('hasSceneSeven').value == 'true'){
				if(document.orderForm.sceneZoomImageReference != null && document.orderForm.sceneZoomImageReference.value != '' && !false){
					zoom_window(document.orderForm.sceneZoomImageReference.value);
				}
				else if(document.orderForm.sceneZoomImageReference != null && document.orderForm.sceneZoomImageReference.value != '' && false){
					rotate_window(document.orderForm.sceneZoomImageReference.value);
				} else {
					remote = open("/largeImage/index.jsp?ab=Product_PageElement_MainProductImage&LargeImageURL=" + escape(document.path.enh.value), "largeImage", "width=505,height=525,left=0,top=0");
				}
			} else {
				remote = open("/largeImage/index.jsp?ab=Product_PageElement_MainProductImage&LargeImageURL=" + escape(document.path.enh.value), "largeImage", "width=505,height=525,left=0,top=0");
			}
		} else {
			remote = open("/largeImage/index.jsp?ab=Product_PageElement_MainProductImage&LargeImageURL=" + escape(document.path.enh.value), "largeImage", "width=505,height=525,left=0,top=0");
		}
		if($$('.capitalize')[0]) {
			remote = open("/largeImage/index.jsp?ab=Product_PageElement_MainProductImage&LargeImageURL=" + enhValue, "largeImage", "width=505,height=525,left=0,top=0");
		}
	}

	function showFlash(){
		document.getElementById('popndiv_cont').style.display='block';
		document.getElementById('panZoomImage').style.display='none';
		document.getElementById('hasSceneSeven').value = "true";
	}
	function swapItReg(REGPath,ENHPath){
		if(!false){
			window.document.prodShot.removeAttribute('height');
			window.document.prodShot.src = REGPath;
			document.path.enh.value = ENHPath;
		} else {
			document.getElementById('hasSceneSeven').value = "false";
			window.document.prodShot.removeAttribute('height');
			window.document.prodShot.src = REGPath;
			document.path.enh.value = ENHPath;

			document.getElementById('popndiv_cont').style.display='none';
			document.getElementById('panZoomImage').style.display='block';
		}
	}
	function checkSizeColor(formObject){
		sizeColorSelected = false;
		if(formObject.prod_0 && formObject.prod_0.type == "select-one" && formObject.prod_0.selectedIndex == 0){
			if(formObject.prod_0.value != ""){
				alert("Please select a "+formObject.prod_0.value+" combination");
			} else {
				var snCheck = 19283636
;
				if(snCheck == "18828656" || snCheck == "18828666")
					alert("Please select a Size / Number combination");
				else {
					alert("Please select a Color / Size combination");
				}
			}
			formObject.prod_0.focus();
			return false;
		}
		return true;
	}
	function hoverSwap(url){
		window.document.prodShot.removeAttribute('height');
		window.document.prodShot.src = url;
	}


	function verifyFields(submitProcess,formObject, isIspu){
		if(notToAdd){
			alert("Item not available in stock.");
			return;
		}

		if(submitProcess == "no"){
			return false;
		} else {
			if(checkSizeColor(formObject)){

		var totalQty = 0;
		var nameStr;
		var nameStrValue;
		showFirstMessage = false;
		showLastMessage = true;
		haveOneError = false;
		qtyField = formObject.qty_0;
		var zipStr;
		nameStr = qtyField.name;
		badZip = "false";

	if(isNaN(parseInt(qtyField.value))){
		showFirstMessage = true;
		haveOneError = true;
	}
	if(!showFirstMessage){
		for (j=0; j < qtyField.value.length; j++){
			if(isNaN(parseInt(qtyField.value.charAt(j)))){
				showFirstMessage = true;
				haveOneError = true;
				break;
			}
		}
	}
	if(showFirstMessage){
		alert(nameStr = " Qty should be a number which is greater than or equal to '1'.");
		qtyField.value = "1";
		qtyField.focus();
		showLastMessage = false;
	} else {
		totalQty = totalQty + parseInt(qtyField.value);
	}
	if(!haveOneError && totalQty > 0){

		if(submitProcess == "custom"){
			if(document.orderForm.prod_0.type == "select-one"){
				document.orderFormCustom.prod_0.value = document.orderForm.prod_0.options[document.orderForm.prod_0.selectedIndex].value;

			} else {
				document.orderFormCustom.prod_0.value = document.orderForm.prod_0.value;
			}
			document.orderFormCustom.quantity.value = document.orderForm.qty_0.value;
			document.orderFormCustom.submit();
		}
		else if(submitProcess == "bundle" || submitProcess == "bundleWish"){
			hasSelected = true;
			formElements = formObject.elements;

			for (i=0; i < formElements.length; i++){
				hasSelected = false;
				if(formElements[i].type == "select-one"){

					if(formElements[i].id=="warranty_0")
					{
						continue;
					}
						if(formElements[i].selectedIndex == 0 && formElements[i].name != "qty_0"){
                              var skuProdId = document.orderForm.prod_0.value;
							if (skuProdId == null || skuProdId == "")
							{
								var skuSelectionValid = validateSkuSelectionForAddToCart();
							}
							else {
								alert("Please select a Color / Size combination");
								formElements[i].focus();
							}

							hasSelected = false;
							break;
						} else {
							hasSelected = true;
						}
				} else if (formElements[i].type == "hidden") {
                    //alert (formElements[i].name + " ::: " + formElements[i].id + " ---- " + formElements[i].value);
                    if (formElements[i].name == "sku" || formElements[i].name == "selectColor" || formElements[i].name == "selectColorIndex" || formElements[i].id == "sku" || formElements[i].id == "selectColor" || formElements[i].id == "selectColorIndex") {
                        if (formElements[i].value == "") {
                            if(window.skuSizeColorObj === undefined) {
                            	hasSelected = true;
                            } else {
                           		alert("Please select a Color / Size combination");
                            	hasSelected = false;
                            	break;
                            }
                        } else {
                            hasSelected = true;
                        }
                    } else {
                        hasSelected = true;
                    }
                } else {
					hasSelected = true;
				}
			}
			if(hasSelected){
				if(submitProcess == "bundleWish")
				formObject.wlName.value = "wlName";
				if (isIspu) {
					return true;
				} else {
					formObject.submit();
				}
			}
		}
		else if(submitProcess == "wishlist"){
			formObject.wlName.value = "wlName";
			if (isIspu) {
				return true;
			} else {
				formObject.submit();
			}
		}
		else if(badZip != "true"){
		formObject.showMy.value = "";
		formObject.wlName.value = "";



			if (isIspu) {
				return true;
			} else {
				formObject.submit();
			}

	}

	}
	else if(showLastMessage){
		alert(nameStr = " Qty should be a number which is greater than or equal to '1'.");
		qtyField.value = "1";
		qtyField.focus();
		showLastMessage = false;
	}

		}
	}

	}
	//Begin - Code modified for NHL under PMO 40044
	function getCatalogURL(baseUrl){
		var rfx_catalog = '1';				//defaut value
		var catalog_name = 'holiday2005';	//defaut value
		var rfx_page = '1';					//defaut value
		regEx = /rfx_catalog=([^&=]*)(&|$)?/
		m=regEx.exec(location.search);
		if(m){
			rfx_catalog = m[1];
		}
		regEx = /catalog_name=([^&=]*)(&|$)?/

		m=regEx.exec(location.search);
		if(m){
			catalog_name = m[1];
		}
		regEx = /rfx_page=([^&=]*)(&|$)?/
		m=regEx.exec(location.search);
		if(m){
			rfx_page = m[1];
		}
		if(rfx_catalog == '1'){
			window.location = baseUrl + catalog_name  + "/?rfx_page=" + rfx_page;
		}
	}
	//End - Code modified for NHL under PMO 40044
	function popUp(URL){
		day = new Date();
		id = day.getTime();
		eval("page" + id + " = window.open(URL, '" + id + "', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width=300,height=50,left = 337,top = 284');");
	}
