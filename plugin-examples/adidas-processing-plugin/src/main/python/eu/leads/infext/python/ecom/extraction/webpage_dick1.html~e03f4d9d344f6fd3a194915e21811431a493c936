			<script language="Javascript">
function swapimageSG(obj)
{
	if(true && (obj.name=='prod_0'))
	{
		var sliceCname_Cid = obj.options[obj.selectedIndex].text.split(':');
		var sliceCname= sliceCname_Cid[0];
		while (sliceCname.indexOf('*') != -1) {
			 sliceCname = sliceCname.replace('*', '');
			}
		sliceCname= sliceCname.replace(/^\s+|\s+$/g, '') ;
		for(var i = 0; i < imageMap_0.length; i++)
		{	
			if((sliceCname.toUpperCase())==((imageMap_0[i].cname).toUpperCase()))
			{
				swapItReg(imageMap_0[i].reg,imageMap_0[i].enh);
				break;
			}
		}
	}
}
</script>

				<script language="JavaScript" type="text/javascript">
				<!--
				var imageMap_0 = new Array();
						imageMap_0[0] = {cname: "Black/Silver", enh:  "/graphics/product_images/pDSP1-15181649dt.jpg",reg: "/graphics/product_images/pDSP1-15181649p275w.jpg"};
