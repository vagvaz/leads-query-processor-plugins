<!-- START TABLET FIXES -->
<script type="text/javascript">
    $(document).ready(function() {
        if(navigator.userAgent.match(/iPad/i) != null || navigator.userAgent.match(/Android/i) != null) {
            $('.navBtn').each(function() {
                if(!$(this).hasClass('navButtonCalendar') && !$(this).hasClass('navButtonOutlet')) {
                    var navIndex = $(this).index('.navBtn');
                    $(this).children('a').attr('href','javascript:setTimeout(toggleNav('+navIndex+'),100);');
                }
            });
        } else {
            $.getScript('/ns/header/js/fl-delay.js',function() {
                console.log('loaded delay script');
                setNDDDelay(0);
            });
        }
    });
    $('.navBtn a').live('touchstart click', function(e) {
        e.stopPropagation();
        var t = $(this);
        $('.sticker_menu').each(function() {
            if($(this).index('.sticker_menu') != t.parent('li').children('.sticker_menu').index('.sticker_menu')) {
                $(this).hide();
            }
        });
    });
    $('.sticker_menu').live('touchstart click', function(e) {
        e.stopPropagation();
    });
    function toggleNav(e) {
        $('.navBtn').eq(e).parent('li').children('.sticker_menu').show();
    }
    $(document).bind('touchstart click', function() {
        $('.sticker_menu').hide();
    });
    var TOUCH_DEVICE = (typeof document.ontouchstart != "undefined") ? true : false;
    if(TOUCH_DEVICE) {
        $("<link/>", {
           rel: "stylesheet",
           type: "text/css",
           href: "/ns/css/fl-tablet-styles.css?cd=0"
        }).appendTo("head");
    }
</script>
<!-- END TABLET FIXES -->

