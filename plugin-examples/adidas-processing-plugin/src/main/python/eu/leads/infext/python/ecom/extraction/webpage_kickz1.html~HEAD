<script type="text/javascript">

	//ProductDetails.disableBackground();

	// color thumbs

	$('.descriptionContainer.chooseColor a img').hover(

        function() {
			var colorSrc = this.src;

			var viewAngle = $(".productDetailPreviewPictures a").index($(".productDetailPreviewPictures a.chooseColorLinkSelected")) + 1;
			if(viewAngle <= 0)
				viewAngle = 1;
			var t = colorSrc.replace( /-.\.jpg$/, '-'+viewAngle+'.jpg' ).replace(/\/30\//, '/288/');
			//alert(t);
			$('img.productDetailPic').attr("src", t);
		},

		function() {
			var viewAngle = $(".productDetailPreviewPictures a").index($(".productDetailPreviewPictures a.chooseColorLinkSelected")) + 1;
			if(viewAngle > 0) {
//			alert(viewAngle + "\n" + $(".productDetailPreviewPictures a.chooseColorLinkSelected img").attr("src").replace(/\/40\//, '/288/') );
				$('img.productDetailPic').attr("src",$(".productDetailPreviewPictures a.chooseColorLinkSelected img").attr("src").replace(/\/40\//, '/288/'));
			} else {
				$('img.productDetailPic').attr("src",$(".productDetailPreviewPictures a:eq(0) img").attr("src").replace(/\/40\//, '/288/'));
			}
		}
	);

    var currentAngleIndex = 1;

    function setNewAngle(next) {

        if(Carousel.not_initialized()) {
            return;
        }

        var index = Carousel.index();
        var newAngle = (next ? index + 1 : index - 1) + 1;

        if(newAngle < 1) {
            newAngle = Carousel.length()
        } else if(newAngle > Carousel.length()) {
            newAngle = 1;
        }

        if(currentAngleIndex != newAngle) {
            currentAngleIndex = newAngle;
            var selLink = $('#angleLink' + newAngle);
            $(".productDetailPreviewPictures a.chooseColorLinkSelected").removeClass("chooseColorLinkSelected");
            selLink.addClass("chooseColorLinkSelected");
            selLink.click();
        }
    }

    $('.productDetailPreviewPictures a img').hover(

        function() {

            if(Carousel.not_initialized()) {
                return;
            }

            var index = $(".productDetailPreviewPictures a").index($(this).parent());
            var newAngle = index + 1;

            if(currentAngleIndex != newAngle) {

//                alert('newAngle = ' + newAngle);
                currentAngleIndex = newAngle;

                $(".productDetailPreviewPictures a.chooseColorLinkSelected").removeClass("chooseColorLinkSelected");
                $(this).parent().addClass("chooseColorLinkSelected");

                Carousel.instance().selectTile(index);
                Carousel.instance().getIndex(index);
                $(this).parent().click();
            }
		},
		function() {
        }
	);

	$('.productDetailPreviewPictures a img').click(
		function() {
			var oldAngle = $(".productDetailPreviewPictures a").index($(".productDetailPreviewPictures a.chooseColorLinkSelected")) + 1;
			var newAngle = $(".productDetailPreviewPictures a").index(this.parentNode) + 1;


			if(oldAngle > 0 && (oldAngle == newAngle)) {
				$(".productDetailPreviewPictures a.chooseColorLinkSelected").removeClass("chooseColorLinkSelected");	// deselect
				$('img.productDetailPic').attr("src",$('img.productDetailPic').attr("src").replace( /-.\.jpg$/, '-'+newAngle+'.jpg' ));
				newAngle = 1;
			} else {
				$(".productDetailPreviewPictures a.chooseColorLinkSelected").attr("class","");
				$(this).parent().addClass("chooseColorLinkSelected");	// select
				$('img.productDetailPic').attr("src",$('img.productDetailPic').attr("src").replace( /-.\.jpg$/, '-'+newAngle+'.jpg' ));
			}

		}
	);

    var zoomImages = new Array (
               "http://media.kickz.com/uk/media/images/p/600/nike-blazer_hi_suede_vintage-royalblue_white-1.jpg",
               "http://media.kickz.com/uk/media/images/p/600/nike-blazer_hi_suede_vintage-royalblue_white-2.jpg",
               "http://media.kickz.com/uk/media/images/p/600/nike-blazer_hi_suede_vintage-royalblue_white-3.jpg",
               "http://media.kickz.com/uk/media/images/p/600/nike-blazer_hi_suede_vintage-royalblue_white-4.jpg",
               "http://media.kickz.com/uk/media/images/p/600/nike-blazer_hi_suede_vintage-royalblue_white-5.jpg",
               "http://media.kickz.com/uk/media/images/p/600/nike-blazer_hi_suede_vintage-royalblue_white-6.jpg"
    );

    var superZoomImages = new Array (
               "http://media.kickz.com/uk/media/images/p/1200/nike-blazer_hi_suede_vintage-royalblue_white-1.jpg",
               "http://media.kickz.com/uk/media/images/p/1200/nike-blazer_hi_suede_vintage-royalblue_white-2.jpg",
               "http://media.kickz.com/uk/media/images/p/1200/nike-blazer_hi_suede_vintage-royalblue_white-3.jpg",
               "http://media.kickz.com/uk/media/images/p/1200/nike-blazer_hi_suede_vintage-royalblue_white-4.jpg",
               "http://media.kickz.com/uk/media/images/p/1200/nike-blazer_hi_suede_vintage-royalblue_white-5.jpg",
               "http://media.kickz.com/uk/media/images/p/1200/nike-blazer_hi_suede_vintage-royalblue_white-6.jpg"
    );

    // big image: click to zoom image
    function show_prod_detail_pic() {
        $('.productBigZoom').show();
        $('.productDetail').addClass('productDetailBigZoom');

        $('.productDetail02').hide();
        $('.advanced_details').hide();

        Carousel.refresh_IE8_initialized();
    }
