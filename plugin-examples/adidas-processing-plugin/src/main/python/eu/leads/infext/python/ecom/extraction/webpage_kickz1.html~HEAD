<script type='text/javascript'>

$(fixWinZIndex = function () {
var zIndexNumber = 1000;
$('div').each(function() {

var divId = $(this).attr('id');
var divClass = $(this).attr('class')

if(divClass=='categoryElementSpecial' || divId=='quantityBar' || divId=='puInBasketBox' || divId=='productPopUpClose') {
    $(this).css('zIndex', 0);
} else {
    $(this).css('zIndex', zIndexNumber);
    zIndexNumber -= 10;
}

});
});
</script>
