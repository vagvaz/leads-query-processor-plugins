    function hideBigZoom () {
        $('.productBigZoom').hide();
        $('.productDetail').removeClass('productDetailBigZoom');
        $('.advanced_details').show();
        $('.productDetail02').show();
    }

    function hideBigZoomPuls () {
        $('.productBigZoomPlus').hide();
        $('.productDetail').removeClass('productDetailBigZoom');
        $('.advanced_details').show();
        $('.productDetail02').show();
    }

