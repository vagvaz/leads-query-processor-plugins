	<script type="text/javascript">
		$(function() {
			var primaryNav      = $('.primary_navigation');
			var allSubmenus     = primaryNav.children('li').children('ul');
			var thirdLevel      = primaryNav.find('.submenu');
			//var firstLevelLink  = primaryNav.children('li').children('a');
			var secondLevelLink = primaryNav.find('li > ul > li');

			secondLevelLink.on('mouseover', function(){
				thirdLevel.hide();
				secondLevelLink.removeClass('active active_prev');
				$(this).prev().addClass('active_prev');
				$(this).addClass('active').children('.submenu').show();
			});
			
			secondLevelLink.on('mouseout', function(){
				thirdLevel.hide();
				secondLevelLink.removeClass('active active_prev');
			})
		});
	</script>
