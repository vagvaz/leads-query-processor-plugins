                            <script type="text/javascript">

                                var count = 0;
                                var imgList = new Array();


                                function imgFound (img) {
                                    imgList.push({"img_path": img.getAttribute("alt")});
                                    checkFinished ();
                                }

                                function imgNotExist (img) {
                                    img.style.display='none';
                                    checkFinished ();
                                }

                                function checkFinished () {
                                    count++;
                                    if(count == 6) {

                                        if(imgList.length > 1) {
                                            Carousel.init({
                                              uuid : "product_carousel",
                                              widgetDir : "/images/carousel/",
                                              args : { "theme" : "white",
                                                       "showNavigation" : true,
                                                       "opacitysetting" : 0.5,
                                                       "itemWidth" : 600,
                                                       "navTop" : 0,
                                                       "scrollInterval" : 500,
                                                       "navigOpacity" : 0.7,
                                                       "cycle" : true
                                                     },
                                              value : imgList
                                            });
                                        } else {
                                            $("#product_carousel").hide();
//                                            $("#product_carousel_template").removeWithDependents();

                                            $("#no_carousel_container").show();
                                        }
                                    }
                                }

                            </script>


