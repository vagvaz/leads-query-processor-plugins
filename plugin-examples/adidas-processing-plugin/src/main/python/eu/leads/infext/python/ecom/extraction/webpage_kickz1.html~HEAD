                                            var popup = $('.CartActionContentHover');
                                            popup.css('top','28px');
                                            popup.css('right','168px');
                                            popup.show();
                                            fixWinZIndex();
                                        },

                                        hide: function() {
                                            $('.CartActionContentHover').hide();
                                            fixWinZIndex();
                                        }
                                    };
