                                    <script type="text/javascript">
                                            ProductDetails.sizesSwitchInit(3);
                                            function changeOption(options) {
                                                var optionId = options.selectedIndex + 1;

                                                for(i = 1; i <= (ProductDetails.sizeMatrixLen + 1); i++) {
                                                   var sizeMatrixDiv =  $('#' + i + 'SizeContainer');
                                                   if(i == optionId) {
                                                       sizeMatrixDiv.show();
                                                   } else {
                                                       sizeMatrixDiv.hide();
                                                   }
                                                }

                                            }
                                        </script>

