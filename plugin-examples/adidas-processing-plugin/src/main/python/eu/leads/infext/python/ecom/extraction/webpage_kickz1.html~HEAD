<script type="text/javascript">
    $(document).ready(function() {

        $('html').click(function(e) {
            if (!$(e.target).parents().andSelf().is('#user_panel')) {
                $("#user_panel > li").removeClass('active').removeClass('blocked').find(".user_panel_submenu").slideUp()
                $("#user_panel").removeClass('active')
            }
        });

        $("#user_panel > li").hover(
                function () {
                    if ($(this).hasClass("active") == false && $(this).hasClass("blocked") == false)
                    {
                        $(this).siblings().removeClass('active').find('.user_panel_submenu').hide()
                        $(this).addClass('active').find('.user_panel_submenu').stop(true,true).slideDown('fast').show()

						$("#user_panel").addClass('active')
					}			
                },function(){
						if ($(this).hasClass("blocked") == false && $("#kickz_id").is(":focus") == false && $("#password").is(":focus") == false)
                    {
						$("#user_panel > li").removeClass('active').find(".user_panel_submenu").slideUp()
	                	$("#user_panel").removeClass('active')
					}
				})
		
        $('#up_language_selector .up_countries').hide();
        $('#up_language_selector ul > li > h4').click(function() {
            $(this).parent().find('.up_countries').toggle();
        });
		$('#registration_expander').click(function(event) {
			event.preventDefault()
            $('#registration_form').toggle();
        });
    });
</script>
